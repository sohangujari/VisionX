{
  "version": 2,
  "builds": [
    {
      "src": "visionx/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "visionx/wsgi.py" }
  ],
  "env": {
    "VERCEL": true,
    "PYTHONUNBUFFERED": "1",
    "STATIC_URL": "/static/",
    "DJANGO_SETTINGS_MODULE": "visionx.settings"
  }
}
